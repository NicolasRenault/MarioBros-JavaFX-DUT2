<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Button?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          prefHeight="400.0" prefWidth="700.0" stylesheets="@Style/MainStylesheet.css">

    <Text text="Nom de sauvegarde : " GridPane.rowIndex="0" />
    <Text text="Nom de sauvegarde" GridPane.rowIndex="0" GridPane.columnIndex="1"/>

    <Text text="Score : " GridPane.rowIndex="1" />
    <Text fx:id="scoreAffichage" GridPane.rowIndex="1" GridPane.columnIndex="1"/>

    <Button fx:id="bouttonSauvegarder" text="Sauvegarder" onAction="#sauvegarde" GridPane.rowIndex="2" GridPane.columnIndex="0"/>

</GridPane>
